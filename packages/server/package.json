{
  "name": "speakrr-server",
  "version": "0.0.1",
  "description": "Speakrr is a podcast management system.",
  "author": "Dieter (coder2000) Lunn",
  "homepage": "https://github.com/coder2000/speakrr#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:coder2000/speakrr.git"
  },
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch | pino-pretty",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:generate",
    "migration:run": "ts-node -r tsconfig-paths/register node_modules/.bin/typeorm migration:run"
  },
  "dependencies": {
    "@nestjs-query/core": "^0.21.2",
    "@nestjs-query/query-graphql": "^0.21.2",
    "@nestjs-query/query-typeorm": "^0.21.2",
    "@nestjs/common": "7.6.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "7.6.5",
    "@nestjs/graphql": "7.9.4",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/typeorm": "7.1.5",
    "apollo-server-express": "^2.19.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^14.7.0",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.1.0",
    "nestjs-pino": "^1.3.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.10.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.6",
    "@nestjs/testing": "^7.6.5",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.9",
    "@types/jest": "26.0.19",
    "@types/node": "^14.14.20",
    "@types/supertest": "^2.0.10",
    "@types/xml2js": "^0.4.7",
    "jest": "^26.6.3",
    "pino-pretty": "^4.3.0",
    "supertest": "^6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
